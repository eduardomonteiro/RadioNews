@model Administrativo.Models.Equipe

@{
    ViewBag.Title = "Equipes";
    ViewBag.Icon = "fa ";
    ViewBag.SmallDescription = "Visualizar detalhes de Equipes";
}
<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<div class="row">
    <div class="col-md-12">
        <div class="box box-info">
            <div class="box-header">
                <h3 class="box-title">Detalhes</h3>
            </div>
            <div class="box-content padded">
                @Html.Partial("_Details")
            </div>
            <div class="box-footer">



                <a href="@Url.Action("Edit","Equipes", new { id = Model.id })" class="btn btn-warning">Editar</a>
                <a href="@Url.Action("Index","Equipes")" class="btn btn-info">Voltar</a>



            </div>
        </div>
    </div>
</div>
