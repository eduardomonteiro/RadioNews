@model Administrativo.Models.EGolPADs

@{
    ViewBag.Title = "Deletar Gol - Painéis Digitais";
    ViewBag.Icon = "fa ";
    ViewBag.SmallDescription = "Deleta um Gol usado nos Painéis Digitais";

}

<div class="box box-danger">
    <div class="box-header">
        <i class="fa fa-trash-o"></i>
        <h3 class="box-title">Deletar</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <div class="callout callout-danger">
            <h4>Atenção</h4>
            <p>Deseja realmente deletar este registro?</p>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <button class="btn btn-danger">Sim</button>
                <a href="@Url.Action("Index", "EGolPaineis")" class="btn btn-success">Não</a>

            }

        </div>
        <div class="box-content padded">

        </div>
    </div>
</div>
