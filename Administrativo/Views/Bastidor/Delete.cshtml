@model Administrativo.Models.Bastidores

@{
    ViewBag.Title = "Album";
    ViewBag.SmallDescription = "Remover um álbum e todas as suas imagens do sistema";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="box box-danger">
    <div class="box-header">
        <i class="fa fa-trash-o"></i>
        <h3 class="box-title">Deletar</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <div class="callout callout-danger">
            <h4>Atenção</h4>
            <p>Deseja realmente deletar este registro?</p>
            @using (Html.BeginForm())
            {
                <button class="btn btn-danger">Sim</button>
                <a href="@Url.Action("Index", "Bastidores")" class="btn btn-success">Não</a>
                    
            }

        </div>
        <div class="box-content padded">
            
        </div>
    </div>
</div>
