@model Administrativo.Models.BastidoresMidias

@{
    ViewBag.Title = "Video da Galeria";
    ViewBag.SmallDescription = "Excluir um vídeo do sistema";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="box box-danger">
    <div class="box-header">
        <i class="fa fa-trash-o"></i>
        <h3 class="box-title">Deletar</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <div class="callout callout-danger">
            <h4>Atenção</h4>
            <p>Deseja realmente deletar este registro?</p>
            @using (Html.BeginForm("VideosDeleteConfirmed", "Bastidor"))
            {
                @Html.HiddenFor(m=>m.id)
                <button class="btn btn-danger">Sim</button>
                <a href="@Url.Action("Videos", "Bastidor")" class="btn btn-success">Não</a>

            }

        </div>
        <div class="box-content padded">

        </div>
    </div>
</div>
